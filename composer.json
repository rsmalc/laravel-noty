{
  "name": "rsmalc/laravel-noty",
  "description": "Flash Messages for Laravel Aplication using Noty v3 library",
  "license": "MIT",
  "authors": [
    {
      "name": "Robert Smalc",
      "email": "robert@smalc.si"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "illuminate/support": "^5.1|^6|^7|^8|^9|^10"
  },
  "autoload": {
    "psr-4": {
      "RSmalc\\": "src/"
    },
    "files": [
      "src/Noty/helpers.php"
    ]
  },
  "minimum-stability": "stable",
  "extra": {
    "laravel": {
      "providers": [
        "RSmalc\\Noty\\NotyServiceProvider"
      ]
    }
  }
}
